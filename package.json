{
  "name": "ms-bot",
  "version": "1.0.0",
  "description": "A private Discord Bot for MS Gaming",
  "type": "module",
  "homepage": "https://github.com/DTrombett/ms-bot#readme",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsup",
    "commands": "npm run build && node --trace-warnings --enable-source-maps ./dist/registerCommands.js",
    "dev": "concurrently \"tsup --watch\" \"wrangler dev\"",
    "fix": "eslint --fix '**/*.js'",
    "lint": "eslint src --fix",
    "ngrok": "ngrok http 8787",
    "prepare": "npm run build",
    "prettier": "prettier --write src/**/*.ts",
    "publish": "wrangler deploy",
    "test": "tsc --noEmit && eslint src"
  },
  "author": "DTrombett <maxtrombdt@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTrombett/ms-bot.git"
  },
  "bugs": {
    "url": "https://github.com/DTrombett/ms-bot/issues"
  },
  "keywords": [],
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@discordjs/formatters": "^0.3.3",
    "@discordjs/rest": "^2.2.0",
    "@sapphire/snowflake": "^3.5.2",
    "discord-api-types": "^0.37.67",
    "htmlparser2": "^9.0.0",
    "mongoose": "^8.0.3",
    "ms": "^2.1.3",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/strictest": "^2.0.2",
    "@types/express": "^4.17.21",
    "@types/ms": "^0.7.34",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.2"
  },
  "optionalDependencies": {
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^6.0.3"
  }
}
