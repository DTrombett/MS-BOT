{
	"name": "ms-bot",
	"version": "1.0.0",
	"description": "A private Discord Bot for MS Gaming",
	"homepage": "https://github.com/DTrombett/ms-bot#readme",
	"exports": "./dist/index.js",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsup",
		"prepare": "npm run build",
		"commands": "npm run build && node --trace-warnings --enable-source-maps ./dist/registerCommands.js",
		"lint": "eslint src --fix",
		"prettier": "prettier --write src/**/*.ts",
		"start": "npm run build && node --trace-warnings --enable-source-maps .",
		"test": "tsc --noEmit && eslint src"
	},
	"author": "DTrombett",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DTrombett/ms-bot.git"
	},
	"bugs": {
		"url": "https://github.com/DTrombett/ms-bot/issues"
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.44",
		"@tsconfig/node-lts": "^18.12.5",
		"discord.js": "^14.13.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"mongoose": "^7.5.2",
		"ms": "^2.1.3",
		"tsup": "^7.2.0",
		"undici": "^5.24.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/ms": "^0.7.31",
		"@types/node": "^18.17.11",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.49.0",
		"eslint-plugin-node": "^11.1.0",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.7",
		"erlpack": "github:discord/erlpack",
		"node": "lts",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.8"
	}
}
