{
  "name": "ms-bot",
  "version": "0.0.1",
  "description": "Official MS Gaming Discord bot",
  "main": "dist/index.js",
  "homepage": "https://github.com/DTrombett/ms-bot#readme",
  "author": "DTrombett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DTrombett/ms-bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTrombett/ms-bot.git"
  },
  "scripts": {
    "test": "eslint src && prettier --check src/**/*.ts && tsc --noEmit",
    "prepare": "npm run test",
    "build": "tsc",
    "lint": "eslint src --fix",
    "prettier": "prettier --write src/**/*.ts",
    "commands": "npm run build && node dist/registerCommands.js",
    "start": "npm run build && node ."
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/collection": "^0.5.0",
    "@discordjs/rest": "^0.3.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^16.11.26",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.28.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "utf-8-validate": "^5.0.8",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  }
}