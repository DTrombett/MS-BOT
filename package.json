{
  "name": "ms-bot",
  "version": "0.0.1",
  "description": "A private Discord Bot for MS Gaming",
  "homepage": "https://github.com/DTrombett/ms-bot#readme",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "commands": "node --experimental-specifier-resolution=node --enable-source-maps --trace-warnings ./dist/registerCommands.js",
    "lint": "eslint src --fix",
    "prepare": "npm test && husky install",
    "prettier": "prettier --write src/**/*.ts",
    "start": "node --experimental-specifier-resolution=node --enable-source-maps --trace-warnings --experimental-fetch .",
    "test": "eslint src && tsc --noEmit"
  },
  "author": "DTrombett",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTrombett/ms-bot.git"
  },
  "bugs": {
    "url": "https://github.com/DTrombett/ms-bot/issues"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0-dev.1648515278-ac4bc3a",
    "@discordjs/rest": "^0.4.0-dev.1648515278-ac4bc3a",
    "bufferutil": "^4.0.6",
    "discord-api-types": "*",
    "discord.js": "^14.0.0-dev.1648515273-ac4bc3a",
    "dotenv": "^16.0.0",
    "erlpack": "github:discord/erlpack",
    "mathjs": "^10.4.2",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3",
    "utf-8-validate": "^5.0.9",
    "workerpool": "^6.2.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@tsconfig/node17": "^1.0.0",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "@types/prettier": "^2.4.4",
    "@types/workerpool": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.4",
    "tsup": "^5.11.13"
  }
}
