{
  "name": "ms-bot",
  "version": "1.0.0",
  "description": "A private Discord Bot for MS Gaming",
  "homepage": "https://github.com/DTrombett/ms-bot#readme",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "commands": "npm run build && node --trace-warnings --enable-source-maps ./dist/registerCommands.js",
    "lint": "eslint src --fix",
    "prettier": "prettier --write src/**/*.ts",
    "start": "npm run build && node --trace-warnings --enable-source-maps .",
    "test": "eslint src && tsc --noEmit"
  },
  "author": "DTrombett",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTrombett/ms-bot.git"
  },
  "bugs": {
    "url": "https://github.com/DTrombett/ms-bot/issues"
  },
  "dependencies": {
    "@discordjs/builders": "*",
    "@discordjs/rest": "*",
    "discord-api-types": "*",
    "discord.js": "^14.0.0-dev.1657239101-e78c9c9",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mathjs": "^10.6.1",
    "ms": "^2.1.3",
    "node": "^18.4.0",
    "undici": "^5.5.1",
    "workerpool": "^6.2.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.0.3",
    "@types/workerpool": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.7.1",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  }
}
