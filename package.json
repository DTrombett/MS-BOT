{
	"name": "ms-bot",
	"version": "1.0.0",
	"description": "A private Discord Bot for MS Gaming",
	"homepage": "https://github.com/DTrombett/ms-bot#readme",
	"exports": "./dist/index.js",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsup",
		"dev": "npm run build && npm start",
		"prepare": "npm run build",
		"commands": "npm run build && node --trace-warnings --enable-source-maps ./dist/registerCommands.js",
		"lint": "eslint src --fix",
		"prettier": "prettier --write src/**/*.ts",
		"start": "node --trace-warnings --enable-source-maps .",
		"test": "tsc --noEmit && eslint src"
	},
	"author": "DTrombett",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DTrombett/ms-bot.git"
	},
	"bugs": {
		"url": "https://github.com/DTrombett/ms-bot/issues"
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.44",
		"@tsconfig/node20": "^20.1.2",
		"discord.js": "^14.13.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"htmlparser2": "^9.0.0",
		"mongoose": "^7.6.3",
		"ms": "^2.1.3",
		"tsup": "^7.2.0",
		"undici": "^5.26.5"
	},
	"devDependencies": {
		"@types/express": "^4.17.20",
		"@types/ms": "^0.7.33",
		"@types/node": "^20.8.8",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "^6.18.1",
		"eslint": "^8.52.0",
		"eslint-plugin-node": "^11.1.0",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.8",
		"erlpack": "github:discord/erlpack",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.8"
	}
}
