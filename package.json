{
	"name": "ms-bot",
	"version": "1.0.0",
	"description": "A private Discord Bot for MS Gaming",
	"homepage": "https://github.com/DTrombett/ms-bot#readme",
	"exports": "./dist/index.js",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsup",
		"prepare": "npm run build",
		"commands": "npm run build && node --trace-warnings --enable-source-maps --experimental-specifier-resolution=node ./dist/registerCommands.js",
		"lint": "eslint src --fix",
		"prettier": "prettier --write src/**/*.ts",
		"start": "npm run build && node --trace-warnings --enable-source-maps --experimental-specifier-resolution=node .",
		"start:prod": "node .",
		"test": "tsc --noEmit && eslint src"
	},
	"author": "DTrombett",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DTrombett/ms-bot.git"
	},
	"bugs": {
		"url": "https://github.com/DTrombett/ms-bot/issues"
	},
	"dependencies": {
		"@discordjs/builders": "*",
		"@discordjs/rest": "*",
		"@tsconfig/node20": "^1.0.1",
		"discord-api-types": "*",
		"discord.js": "^14.11.0",
		"dotenv": "^16.1.4",
		"express": "^4.18.2",
		"ms": "^2.1.3",
		"tsup": "^6.7.0",
		"undici": "^5.22.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/ms": "^0.7.31",
		"@types/node": "^20.2.5",
		"@types/workerpool": "^6.4.0",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"eslint": "^8.45.0",
		"eslint-plugin-node": "^11.1.0",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.7",
		"erlpack": "github:discord/erlpack",
		"node": "^20.2.0",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.8"
	}
}
